<sdf version="1.5">
  <world name="default">

    <!-- Martian Gravity-->
    <physics type='ode'>
      <max_step_size>0.01</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>100</real_time_update_rate>
      <gravity>0 0 -3.711</gravity>
    </physics>
    
    <include>
      <uri>model://sun</uri>
    </include>

    <include>
      <uri>model://hmap</uri>
      <name>hmap</name>
      <pose>0 0 0 0 0 0</pose>
    </include>

  </world>
</sdf>